          <argument>
            <kind>1</kind>
            <string>///Handle Camera Effects

/* -----------------------------------------
   Movement
----------------------------------------- */

if (instance_exists(_view_object)) {
    
    var _xview, _yview;
    
    if (_bound_x == -1 and _bound_y == -1 and _bound_width == -1 and _bound_height == -1) {
    
        x = _view_object.x;
        y = _view_object.y;
        
        _xview = (_view_object.x - view_wview[_view] / 2)+_view_x_offset+_shake_effect_x;
        _yview = (_view_object.y - view_hview[_view] / 2)+_view_y_offset+_shake_effect_y;
        
    } else {
 
        x = clamp(_view_object.x, _bound_x+view_wview[_view] / 2, _bound_x+_bound_width-(view_wview[_view]/2));
        y = clamp(_view_object.y, _bound_y+view_hview[_view] / 2, _bound_y+_bound_height-(view_hview[_view]/2));
        
        _xview = ( (x - (view_wview[_view] / 2))+_view_x_offset+_shake_effect_x );
        _yview = ( (y - (view_hview[_view] / 2))+_view_y_offset+_shake_effect_y );
    
    }
    
    view_xview[_view] = _xview;
    view_yview[_view] = _yview;

} else {

    view_xview[_view] = (x - view_wview[_view] / 2)+_view_x_offset+_shake_effect_x;
    view_yview[_view] = (y - view_hview[_view] / 2)+_view_y_offset+_shake_effect_y;    

}

/* -----------------------------------------
   Scroll
----------------------------------------- */

if (_fx_scroll == true) {
    
    _scroll_time+= 1;
    
    if (_scroll_easing == -1) {
        _view_x_offset = lerp(_scroll_start_x, _scroll_end_x, ex_camera_smoothstep(0, 1, _scroll_time / _scroll_duration));
        _view_y_offset = lerp(_scroll_start_y, _scroll_end_y, ex_camera_smoothstep(0, 1, _scroll_time / _scroll_duration));
    } else {
        _view_x_offset = lerp(_scroll_start_x, _scroll_end_x, script_execute(_scroll_easing, _scroll_time, 0, clamp(_scroll_time * _scroll_duration, 0, 1), _scroll_duration));
        _view_y_offset = lerp(_scroll_start_y, _scroll_end_y, script_execute(_scroll_easing, _scroll_time, 0, clamp(_scroll_time * _scroll_duration, 0, 1), _scroll_duration));
    }

    if (_scroll_time &gt;= _scroll_duration) {
        _fx_scroll = false;
    }

}


/* -----------------------------------------
   Zoom
----------------------------------------- */

if (_fx_zoom == true) {
    
    _zoom_time+= 1;
    
    if (_zoom_easing == -1) {
        _zoom = lerp(_zoom_start, _zoom_end, ex_camera_smoothstep(0, 1, _zoom_time / _zoom_duration));
    } else {
        _zoom = lerp(_zoom_start, _zoom_end, script_execute(_zoom_easing, _zoom_time, 0, clamp(_zoom_time * _zoom_duration, 0, 1), _zoom_duration));
    }
    
    view_wview[_view] = view_wport[_view]*(_zoom / 100);
    view_hview[_view] = view_hport[_view]*(_zoom / 100);
    
    if (_zoom_time &gt;= _zoom_duration) {
        _fx_zoom = false;
    }

}

/* -----------------------------------------
   Tilt
----------------------------------------- */

if (_fx_tilt == true) {
    
    _tilt_time+= 1;
    
    if (_tilt_easing == -1) {
        _tilt = lerp(_tilt_start, _tilt_end, ex_camera_smoothstep(0, 1, _tilt_time / _tilt_duration));
    } else {
        _tilt = lerp(_tilt_start, _tilt_end, script_execute(_tilt_easing, _tilt_time, 0, clamp(_tilt_time * _tilt_duration, 0, 1), _tilt_duration));
    }
    
    view_angle[_view] = _tilt;
    
    if (_tilt_time &gt;= _tilt_duration) {
        _fx_tilt = false;
    }

}

/* -----------------------------------------
   Fade
----------------------------------------- */

if (_fx_fade == true) {
    
    if (_fade_complete == false) {

        _fade_time+= 1;
        
        if (_fade_easing == -1) {
            _fade_alpha = lerp(_fade_start, _fade_end, ex_camera_smoothstep(0, 1, _fade_time / _fade_duration));
        } else {
            _fade_alpha = lerp(_fade_start, _fade_end, script_execute(_fade_easing, _fade_time, 0, clamp(_fade_time * _fade_duration, 0, 1), _fade_duration));
        }
    
    }
    
    if (_fade_time &gt;= _fade_duration) {
        
        if (_fade_ext == true) {
            
            _fade_complete = true;
            _fade_ext_time+= 1;
            ex_camera_set_fade_alpha(id, 1.0);
            
            if (_fade_ext_time &gt;= _fade_delay) {
                _fx_fade = false;
                _fade_complete = false;
                room_goto(_fade_room);
            }
            
        } else {
            _fade_complete = false;
            _fx_fade = false;
        }
    }

}

/* -----------------------------------------
   Flash
----------------------------------------- */

if (_fx_flash == true) {
    
    _flash_time+= 1;
    
    if (_flash_easing == -1) {
        _flash_alpha = lerp(_flash_start, _flash_end, ex_camera_smoothstep(0, 1, _flash_time / _flash_duration));
    } else {
        _flash_alpha = lerp(_flash_start, _flash_end, script_execute(_flash_easing, _flash_time, 0, clamp(_flash_time * _flash_duration, 0, 1), _flash_duration));
    }
    
    if (_flash_time &gt;= _flash_duration) {
        _fx_flash = false;
    }

}


/* -----------------------------------------
   Shake
----------------------------------------- */

if (_fx_shake == true) {
    
    _shake_time+= 1;
    
    if (_shake_power_x &gt; 0) {
    
        if (_shake_easing == -1) {
            _shake_temp_x = _shake_power_x;
        } else {
            _shake_temp_x = lerp(_shake_power_x, 0, script_execute(_shake_easing, _shake_time, 0, clamp(_shake_time * _shake_duration, 0, 1), _shake_duration));
        }
        _shake_effect_x = random_range(-_shake_temp_x, _shake_temp_x);
        
    }
    
    if (_shake_power_y &gt; 0) {
    
        if (_shake_easing == -1) {
            _shake_temp_y = _shake_power_y;
        } else {
            _shake_temp_y = lerp(_shake_power_y, 0, script_execute(_shake_easing, _shake_time, 0, clamp(_shake_time * _shake_duration, 0, 1), _shake_duration));
        }
        _shake_effect_y = random_range(-_shake_temp_y, _shake_temp_y);
        
    }
    
    if (_shake_power_angle &gt; 0) {
    
        if (_shake_easing == -1) {
            _shake_temp_angle = lerp(_shake_power_angle, 0, ex_camera_smoothstep(0, 1, _shake_time / _shake_duration));
        } else {
            _shake_temp_angle = lerp(_shake_power_angle, 0, script_execute(_shake_easing, _shake_time, 0, clamp(_shake_time * _shake_duration, 0, 1), _shake_duration));
        }
        view_angle[_view] = _tilt + random_range(-_shake_temp_angle, _shake_temp_angle);   
    }
    
    if (_shake_time &gt;= _shake_duration) {
        view_angle[_view] = _tilt;
        _shake_effect_x = 0;
        _shake_effect_y = 0;
        _fx_shake = false;
    }

}



</string>
